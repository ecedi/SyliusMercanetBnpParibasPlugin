{
    "name": "ecedi/mercanet-bnp-paribas-plugin",
    "type": "sylius-plugin",
    "description": "Mercanet BNP Paribas payment plugin for Sylius applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikołaj Król",
            "email": "mikolaj.krol@bitbag.pl"
        },
        {
            "name": "Patryk Drapik",
            "email": "patryk.drapik@bitbag.pl"
        }
    ],
    "require": {
        "php": "^7.3",

        "sylius/sylius": "^1.8"
    },
    "require-dev": {
        "behat/behat": "^3.7",
        "behat/mink-selenium2-driver": "^1.4",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/service-container-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^6.2",
        "phpunit/phpunit": "^8.5",
        "se/selenium-server-standalone": "^2.52"
    },
    "autoload": {
        "psr-4": {
            "BitBag\\MercanetBnpParibasPlugin\\": "src/",
            "Tests\\BitBag\\MercanetBnpParibasPlugin\\": "tests/"
        },
        "exclude-from-classmap": [
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4" : {

        },
        "classmap": [
            "tests/Application/app/AppKernel.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    }
}
